@model IEnumerable<Mirosnicenco_Eugenia_Proiect.Models.PredictionHistory>

<h2>Prediction History</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Date</th>
            <th>Country</th>
            <th>Household Size</th>
            <th>Income Level</th>
            <th>Urban / Rural</th>
            <th>Adoption Year</th>
            <th>Predicted Monthly Consumption (kWh)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CreatedAt.ToString("yyyy-MM-dd HH:mm")</td>
                <td>@item.Country</td>
                <td>@item.Household_Size</td>
                <td>@item.Income_Level</td>
                <td>@item.Urban_Rural</td>
                <td>@item.Adoption_Year</td>
                <td>@item.Monthly_Usage_kWh</td>
            </tr>
        }
    </tbody>
</table>
